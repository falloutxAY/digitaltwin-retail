ore;1"},{"@type":"Relationship","name":"contains","description":"Defines the areas which are located in the floor","target":"dtmi:digitaltwins:retail:Area;1"}]},{"@context":["dtmi:dtdl:context;2"],"@id":"dtmi:digitaltwins:retail:AreaCheckout;1","extends":"dtmi:digitaltwins:retail:Area;1","@type":"Interface","displayName":"Checkout Area","description":"Checkout/cashier area"},{"@context":["dtmi:dtdl:context;2"],"@id":"dtmi:digitaltwins:retail:AreaDisplay;1","extends":"dtmi:digitaltwins:retail:Area;1","@type":"Interface","displayName":"Display Area","description":"Display areas for products can be segmented. E.g. Fruits area."},{"@context":["dtmi:dtdl:context;2"],"@id":"dtmi:digitaltwins:retail:Aisle;1","@type":"Interface","displayName":"Aisle","description":"An aisle that is in a store","contents":[{"@type":"Property","name":"aisleName","displayName":"Aisle Name","description":"Name of an aisle. E.g. Aisle 6","schema":"string"},{"@type":"Component","name":"areaCoordinates","description":"X-Y-Z coodinate of the area location","schema":"dtmi:digitaltwins:retail:SpaceCoordinates;1"},{"@type":"Property","name":"aisleLength","displayName":"Aisle Length","description":"Length of aisle measured in meters","schema":"double"},{"@type":"Relationship","name":"isPartOf","description":"Defines the relationship where this store is located in a display area.","target":"dtmi:digitaltwins:retail:AreaDisplay;1"},{"@type":"Relationship","name":"containis","description":"Defines the relationship where aisle has endcap","target":"dtmi:digitaltwins:retail:endcap;1"}]},{"@context":["dtmi:dtdl:context;2"],"@id":"dtmi:digitaltwins:retail:DisplaySection;1","@type":"Interface","displayName":"Display Section","description":"Aisle is divided in rows and section","comment":"Roxs are divided into sections","contents":[{"@type":"Property","name":"sectionName","displayName":"Section Name","description":"E.g. Row 2 of isle 6, section 1","schema":"string"},{"@type":"Component","name":"areaCoordinates"